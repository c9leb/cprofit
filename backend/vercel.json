{
  "builds": [
    {
      "src": "/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/webhooks/update",
      "dest": "/webhooks.js",
      "methods": ["POST"]
    },
    {
      "src": "/webhooks/product",
      "dest": "/webhooks.js"
    },
    {
      "src": "/",
      "dest": "/server.js"
    }
  ]
}